#include <stdio.h>
int main(void)
{
    int a[5]={0,};
    int b[5]={0,};
    int i;
    int na,nb;
    for(i=0;i<4;i++)
    {
        scanf("%c",&a[i]);
        a[i]=a[i]-'0';
    }
    for(i=0;i<3;i++)
    {
        scanf("%c",&b[i]);
        b[i]=b[i]-'0';
    }
    
    na=a[0]+10*a[1]+100*a[2];
    nb=b[0]+10*b[1]+100*b[2];
    if(na>nb)
    {
        printf("%d",na);
    }else
    {
        printf("%d",nb);
    }
    return 0;
}
